        var sync1 = $("#item-images");
        var sync2 = $("#item-thumbs");
        var slidesPerPage = 4; //globaly define number of elements per page
        var syncedSecondary = true;

        sync1.owlCarousel({
            items: 1,
            loop: true,
            smartSpeed: 1000,
            nav: true,
            dots: false,
            responsiveRefreshRate: 200,
            responsive: {
                0: {
