        $('.page-description').addClass('is_closed');
    } else if ($(window).width() <= 767 && fullHeight > 130) {
        $('.page-descr-content').css('max-height', '109px');
        $('.page-description').addClass('is_closed');
    } else if ($(window).width() > 767 && fullHeight > 140) {
        $('.page-descr-content').css('max-height', '120px');
        $('.page-description').addClass('is_closed');
    }

