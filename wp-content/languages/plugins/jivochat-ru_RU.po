#: templates/signup.php:171
msgid "Already have an account? <span id=\"login_link\" class=\"jivosite-redirect\">Log in</span>"
msgstr "Уже есть аккаунт? <span id=\"login_link\" class=\"jivosite-redirect\">Авторизоваться</span>"

#: templates/signup.php:152
msgid "By creating an account you agree to <a class=\"jivosite-caption\" href=\"https://www.jivochat.com/terms?utm_source=wordpress\" target=\"_blank\">Terms and Conditions</a> and <a class=\"jivosite-caption\" href=\"https://www.jivochat.com/files/privacy_policy.pdf?utm_source=wordpress\" target=\"_blank\">Privacy Policy</a>"
